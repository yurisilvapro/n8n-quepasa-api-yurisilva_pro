{
  "name": "n8n-nodes-quepasa",
  "version": "2.0.0",
  "description": "Complete n8n integration for QuePasa WhatsApp API - 48 operations, multi-account support, 100% API v4 coverage",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "whatsapp",
    "quepasa",
    "whatsapp-api",
    "automation",
    "workflow",
    "typescript",
    "multi-account"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yurisilvapro/n8n-quepasa-api-yurisilva_pro",
  "author": {
    "name": "Yuri Silva",
    "email": "yuri@yurisilva.pro"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yurisilvapro/n8n-quepasa-api-yurisilva_pro.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy:assets",
    "copy:assets": "copyfiles -u 1 \"nodes/**/*.{png,svg,json}\" dist/",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/QuePasaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/QuePasa/QuePasa.node.js",
      "dist/nodes/QuePasaTrigger/QuePasaTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.24.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "form-data": "^4.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
